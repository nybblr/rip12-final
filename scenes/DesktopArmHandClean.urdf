<include filename="table2b.urdf" model_name="table2b"/>
<include filename="bolt.urdf" model_name="bolt"/>
<include filename="block.urdf" model_name="block"/>
<include filename="driver.urdf" model_name="driver"/>

<!-- Construct the world -->
