</entity>

<entity model="bolt">
	<origin xyz="-.58 1.33 .81" rpy="0 0 0"/>
</entity>

<entity model="block">
	<origin xyz="0 0.91 1.11" rpy="-1.57 0 0"/>
</entity>

<entity model="driver">
	<origin xyz="-.71 1.33 .87" rpy="-1.57 0 0"/>
</entity>

</world>
